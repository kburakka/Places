# platform :ios, '9.0'


def the_pods
  pod 'Alamofire', '~> 4.9.1'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'SDWebImage', :modular_headers => true
  pod "PromiseKit", "~> 6.8"
end

target 'MARTI' do
	the_pods
end

target 'MARTITests' do
	the_pods
end
